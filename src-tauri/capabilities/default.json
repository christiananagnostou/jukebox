{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "main-capability",
  "description": "Default capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:default",
    "fs:default",
    "fs:allow-home-read-recursive",
    "fs:allow-home-meta-recursive",
    "sql:default",
    "sql:allow-execute"
  ]
}
